    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "prettier": "^2.2.1"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "5.2.0",
    "typescript": "4.1.3"
  },
  "proxy": "http://localhost:5000"
