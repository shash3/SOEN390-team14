    "jquery": "3.5.1",
    "jwt-decode": "^3.1.2",
    "moment": "2.29.1",
    "node-sass": "4.14.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "14.6.3",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "^4.0.1",
    "reactstrap": "8.9.0"
