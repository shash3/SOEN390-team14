{
    "name": "run-all",
    "version": "1.0.0",
    "scripts": {
        "server": "nodemon server --prefix back-end",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server-dependencies": "npm run install --prefix back-end",
        "client-dependencies": "npm install --prefix client",
        "install-all": "concurrently \"npm run server-dependencies\" \"npm run client-dependencies\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shash3/SOEN390-team14.git"
    },
